<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="sogo.malba.soapcliente.Controlador">
    <children>
        <TextField fx:id="nameField" promptText="Nombre del Personaje"/>
        <Button fx:id="getButton" text="Get Personaje"/>
        <Button fx:id="randomButton" text="Get Random Personaje"/>
        <TextArea fx:id="resultArea" editable="false"/>
    </children>
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <spacing>10</spacing>
</VBox>



